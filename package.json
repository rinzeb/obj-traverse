    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "chai": "3.4.1",
    "eslint": "1.7.2",
    "eslint-loader": "1.1.0",
    "mocha": "2.3.4",
    "webpack": "1.12.9",
    "yargs": "3.32.0"
